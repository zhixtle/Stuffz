@model IEnumerable<Pastebook.Models.FriendModel>

<div class="content-main">
    <div class="row margin-b-30">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="content-item">
                    <div id="friendsList">
                        <div class="friends-head">
                            <h2>Friends</h2>
                        </div>
                        <div class="friends-body">
                            @foreach (var item in Model)
                            {
                                <div class="friends-item">
                                    <a href="@Url.Action("ViewProfile", "UserProfile", new { profileUsername = Html.DisplayFor(friend => item.Username)})">
                                        @Html.DisplayFor(friend => item.FirstName) @Html.DisplayFor(friend => item.LastName)
                                    </a>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

